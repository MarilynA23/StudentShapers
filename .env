# Ollama URL for the backend to connect
# The path '/ollama' will be redirected to the specified backend URL
# This value is ignored (and replaced) if ENABLE_GHOSTWHEEL_API is set to true
OLLAMA_BASE_URL='http://localhost:11434'

AZURE_OPENAI_API_VERSION="VERSION"
AZURE_OPENAI_CHAT_DEPLOYMENT_NAME="DEPLOYMENT-NAME"
AZURE_OPENAI_ENDPOINT="ENDPOINT"
AZURE_OPENAI_API_KEY="API-KEY"
EMBEDDING_MODEL_NAME="MODEL-NAME"
#AUTOMATIC1111_BASE_URL="http://localhost:7860"

# DO NOT TRACK
SCARF_NO_ANALYTICS=true
DO_NOT_TRACK=true
ANONYMIZED_TELEMETRY=false

# Disable user login (remove if not running locally)
WEBUI_AUTH=false
ENABLE_SIGNUP=false

# Enable the ghostwheel backend and provide your API key
ENABLE_GHOSTWHEEL_API=true
GHOSTWHEEL_API_KEY="your ghostwheel api key"
